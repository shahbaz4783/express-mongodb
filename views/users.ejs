<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<title>Document</title>
	</head>
	<body>
		<h1>All Users</h1>
		<table>
			<thead>
				<th>S.No</th>
				<th>Name</th>
				<th>Age</th>
				<th>User ID</th>
				<th>Email</th>
			</thead>
			<tbody>
				<% users.forEach((user, index) => { %>
				<tr>
					<td><%= index + 1 %></td>
					<td>
						<a href="/users/<%= user._id %>"> <%= user.name %> </a>
					</td>
					<td><%= user.age %></td>
					<td><%= user.userID %></td>
					<td><%= user.email %></td>
				</tr>
				<% }) %>
			</tbody>
		</table>
	</body>
</html>
